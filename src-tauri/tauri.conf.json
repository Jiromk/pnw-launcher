{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "com.pnw.launcher",
  "productName": "PNW Launcher",
  "version": "1.0.0",
  "build": {
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "vite build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "security": { "csp": null },
    "windows": [
      {
        "label": "main",
        "title": "Pokémon New World — Launcher",
        "width": 980,
        "height": 640,
        "resizable": true
      }
    ]
  },
  "bundle": {},
  "plugins": {}
}
